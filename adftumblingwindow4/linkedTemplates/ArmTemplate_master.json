					},
					"DynamicLinkedserver_properties_typeProperties_server": {
						"value": "[parameters('DynamicLinkedserver_properties_typeProperties_server')]"
					},
					"DynamicLinkedserver_properties_typeProperties_database": {
						"value": "[parameters('DynamicLinkedserver_properties_typeProperties_database')]"
					},
					"DynamicLinkedserver_properties_typeProperties_userName": {
						"value": "[parameters('DynamicLinkedserver_properties_typeProperties_userName')]"
					},
					"linkedserversqlserver3_properties_typeProperties_server": {
						"value": "[parameters('linkedserversqlserver3_properties_typeProperties_server')]"
					},
					"linkedserversqlserver3_properties_typeProperties_database": {
						"value": "[parameters('linkedserversqlserver3_properties_typeProperties_database')]"
					},
					"linkedserversqlserver3_properties_typeProperties_userName": {
						"value": "[parameters('linkedserversqlserver3_properties_typeProperties_userName')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					},
					"SqlServerLinkedServernew_properties_typeProperties_server": {
						"value": "[parameters('SqlServerLinkedServernew_properties_typeProperties_server')]"
					},
					"SqlServerLinkedServernew_properties_typeProperties_database": {
						"value": "[parameters('SqlServerLinkedServernew_properties_typeProperties_database')]"
					},
					"SqlServerLinkedServernew_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerLinkedServernew_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
